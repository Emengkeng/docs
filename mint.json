{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Even Top",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#18181b",
    "light": "#71717a",
    "dark": "#18181b",
    "background": {
      "dark": "#09090b",
