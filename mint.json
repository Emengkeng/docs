{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Even Top",
  "logo": {
    "dark": "/logo/eventop-logo-white.png",
    "light": "/logo/eventop-logo-black.png"
  },
  "favicon": "/favicon.ico",
  "colors": {
    "primary": "#18181b",
    "light": "#71717a",
    "dark": "#18181b",
    "background": {
      "dark": "#09090b",
      "light": "#ffffff"
    },
    "anchors": {
      "from": "#18181b",
      "to": "#71717a"
    }
  },
  "topbarLinks": [
    {
      "name": "Dashboard",
      "url": "https://dashboard.eventop.xyz"
    }
  ],
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://eventop.xyz"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    },
    {
      "name": "Merchants",
      "url": "merchants"
    },
    {
      "name": "Users",
      "url": "users"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": [
        "introduction",
        "quickstart"
      ]
    },
    {
      "group": "Merchants",
      "pages": [
        "merchants/overview",
        "merchants/quickstart",
        "merchants/how-it-works",
        "merchants/integration-guide",
        "merchants/testing",
        "merchants/go-live"
      ]
    },
    {
      "group": "Integration",
      "pages": [
        "integration/webhooks",
        "integration/sdks"
      ]
    },
    {
      "group": "API Reference",
      "pages": [
        "api-reference/introduction",
        "api-reference/authentication",
        "api-reference/checkout",
        "api-reference/subscriptions",
        "api-reference/customers"
      ]
    },
    {
      "group": "Users",
      "pages": [
        "users/getting-started",
        "users/mobile-app"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://x.com/tryEventop",
    "github": "https://github.com/Emengkeng"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "backgroundImage": "/images/background.png",
  "modeToggle": {
    "default": "light",
    "isHidden": false
  }
}